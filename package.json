    "eslint": "8.11.0",
    "sass": "1.49.9",
    "typescript": "4.6.2",
    "unocss": "0.33.0",
    "unplugin-auto-import": "0.6.4",
    "unplugin-vue-components": "0.19.6",
    "vite": "2.9.8",
    "vite-plugin-md": "0.11.9",
    "vite-plugin-pages": "0.21.4",
    "vite-plugin-vue-images": "0.6.1",
    "vite-plugin-vue-layouts": "0.6.0",
