    "typescript": "4.5.5",
    "unocss": "0.25.0",
    "unplugin-auto-import": "0.6.0",
    "unplugin-vue-components": "0.17.18",
    "vite": "2.8.4",
    "vite-plugin-md": "0.11.8",
    "vite-plugin-pages": "0.20.2",
    "vite-plugin-vue-images": "0.6.1",
